{
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.5",
    "@core/asyncutil": "jsr:@core/asyncutil@^1.0.2",
    "@eluxlab/library-components": "https://esm.sh/@eluxlab/library-components@0.3.7?external=react,react-dom,react/jsx-runtime&target=es2022",
    "@hono/hono": "jsr:@hono/hono@^4.5.4",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "@std/assert": "jsr:@std/assert@^1.0.2",
    "@std/async": "jsr:@std/async@^0.224.1",
    "@std/cli": "jsr:@std/cli@^1.0.3",
    "@std/crypto": "jsr:@std/crypto@1.0.0-rc.1",
    "@std/datetime": "jsr:@std/datetime@^0.224.0",
    "@std/encoding": "jsr:@std/encoding@^1.0.0-rc.1",
    "@std/flags": "jsr:@std/flags@^0.224.0",
    "@std/fmt": "jsr:@std/fmt@^0.225.3",
    "@std/fs": "jsr:@std/fs@^0.229.1",
    "@std/http": "jsr:@std/http@^1.0.0",
    "@std/io": "jsr:@std/io@^0.224.4",
    "@std/log": "jsr:@std/log@^0.224.5",
    "@std/media-types": "jsr:@std/media-types@^1.0.0-rc.1",
    "@std/path": "jsr:@std/path@^0.225.2",
    "@std/semver": "jsr:@std/semver@^0.224.3",
    "@std/streams": "jsr:@std/streams@^1.0.0",
    "@std/testing": "jsr:@std/testing@^1.0.0",
    "@zaubrik/djwt": "jsr:@zaubrik/djwt@^3.0.2",
    "daisyui": "npm:daisyui@4.4.19",
    "fast-json-patch": "npm:fast-json-patch@^3.1.1",
    "https://esm.sh/*preact-render-to-string@6.3.1": "npm:preact-render-to-string@6.4.2",
    "partytown/": "https://deno.land/x/partytown@0.4.8/",
    "preact": "npm:preact@10.23.1",
    "preact-render-to-string": "npm:preact-render-to-string@6.4.2",
    "react": "npm:preact@10.23.1/compat",
    "react-dom": "npm:preact@10.23.1/compat",
    "simple-git": "npm:simple-git@^3.25.0",
    "site/": "./",
    "std/": "https://deno.land/std@0.190.0/",
    "update": "npm:update@^0.7.4",
    "@deco/codemod-toolkit": "jsr:@deco/codemod-toolkit@0.3.4",
    "@deco/deco": "jsr:@deco/deco@1.128.2",
    "@deco/deno-ast-wasm": "jsr:@deco/deno-ast-wasm@^0.5.5",
    "@deco/dev": "jsr:@deco/dev@1.128.2",
    "@deco/durable": "jsr:@deco/durable@0.5.3",
    "@deco/warp": "jsr:@deco/warp@0.3.10",
    "apps/": "https://cdn.jsdelivr.net/gh/deco-cx/apps@0.128.16/",
    "deco/": "https://cdn.jsdelivr.net/gh/deco-cx/deco@1.128.2/",
    "drizzle-orm": "https://esm.sh/drizzle-orm@0.30.10",
    "drizzle-orm/": "https://esm.sh/drizzle-orm@0.30.10/",
    "drizzle-kit": "https://esm.sh/drizzle-kit@0.21.2"
  },
  "tasks": {
    "start": "deno run -A --unstable-http --env https://deco.cx/run -- deno task dev",
    "gen": "deno run -A dev.ts --gen-only",
    "play": "USE_LOCAL_STORAGE_ONLY=true deno task start",
    "component": "deno eval 'import \"deco/scripts/component.ts\"'",
    "release": "deno eval 'import \"deco/scripts/release.ts\"'",
    "update": "deno run -Ar https://deco.cx/update",
    "check": "deno fmt && deno lint && deno check dev.ts main.ts",
    "install": "deno eval 'import \"deco/scripts/apps/install.ts\"'",
    "uninstall": "deno eval 'import \"deco/scripts/apps/uninstall.ts\"'",
    "bundle": "deno run -A jsr:@deco/deco/scripts/bundle site",
    "cache_clean": "rm deno.lock; deno cache -r main.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "dev": "deno run -A --env --unstable-kv --unstable-hmr dev.ts",
    "reload": "deno cache -r https://deco.cx/run",
    "format": "deno fmt",
    "db:setup:deps": "deno cache -r --node-modules-dir npm:drizzle-orm@0.30.10 npm:drizzle-kit@0.21.2 npm:@libsql/client@0.6.0/node",
    "db:check:credentials": "deno eval --env 'import{getDbCredentials}from\"apps/records/scripts/checkDbCredential.ts\";getDbCredentials();'",
    "db:pull:prod": "deno eval --env 'import \"apps/records/scripts/pullProd.ts\"'",
    "db:generate": "deno run -A --node-modules-dir npm:drizzle-kit@0.21.2 generate < /dev/null",
    "db:migrate": "deno run -A --node-modules-dir --config=deno.json npm:drizzle-kit@0.21.2 migrate",
    "db:drop": "deno run -A --node-modules-dir --config=deno.json npm:drizzle-kit@0.21.2 drop",
    "db:schema:update": "DENO_FUTURE=1 deno task db:check:credentials && deno task db:generate && deno task db:migrate"
  },
  "githooks": {
    "pre-commit": "check"
  },
  "exclude": [
    "node_modules",
    "static/",
    "README.md",
    "_fresh",
    "**/_fresh/*",
    ".deco/**"
  ],
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 80,
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": false
  },
  "nodeModulesDir": false,
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "lock": false
}
